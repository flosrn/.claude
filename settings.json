{
  "includeCoAuthoredBy": false,
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "bun /Users/flo/.claude/scripts/command-validator/src/cli.ts"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "afplay -v 0.1 /Users/flo/.claude/song/finish.mp3"
          }
        ]
      }
    ],
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "afplay -v 0.1 /Users/flo/.claude/song/need-human.mp3"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Edit|Write|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "bun /Users/flo/.claude/scripts/hook-post-file-enhanced.ts"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "bun /Users/flo/.claude/scripts/statusline/src/index.ts",
    "padding": 0
  },
  "enabledPlugins": {
    "frontend-design@claude-code-plugins": true
  }
}
